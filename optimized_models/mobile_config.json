{
  "model": {
    "use_quantized": true,
    "max_sequence_length": 256,
    "batch_size": 1,
    "num_attention_heads": 4,
    "hidden_dim": 256,
    "num_layers": 4,
    "dropout": 0.0
  },
  "tokenizer": {
    "vocab_size": 1024,
    "max_audio_tokens": 64,
    "max_imu_tokens": 128
  },
  "inference": {
    "enable_caching": true,
    "use_torch_jit": false,
    "threading": {
      "num_threads": 2,
      "enable_parallel": false
    }
  },
  "memory": {
    "max_buffer_size": 1024,
    "enable_gradient_checkpointing": false,
    "clear_cache_interval": 100
  }
}